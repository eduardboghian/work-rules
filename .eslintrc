{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "no-plusplus": 0,
        "eqeqeq": 0,
        "no-case-declarations": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4
            }
        ]
    }
}